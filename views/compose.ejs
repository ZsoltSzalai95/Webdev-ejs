<%- include("partials/header") -%>
<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label for="">Title</label><br />
    <textarea class="form-control" name="title" rows="2" cols="150"></textarea><br />


    <label for="">Post</label><br />
    <textarea class="form-control" name="post" rows="10" cols="150"></textarea><br />

    <button type="submit" name="button" class="btn btn-primary">Publish</button>
  </div>

</form>


